---
import Alert from './Alert/Alert.astro';
import CodeBlock from './CodeBlock/CodeBlock.astro';
import CodeSnippet from './CodeSnippet/CodeSnippet.astro';
import Emphasis from './Emphasis/Emphasis.astro';
import GithubRepo from './GithubRepo/GithubRepo.astro';
import Heading from './Heading/Heading.astro';
import ItemsList from './ItemsList/ItemsList.astro';
import ItemsListEntry from './ItemsList/ItemsListEntry/ItemsListEntry.astro';
import { wrapper } from './MarkdownComponents.css';
import ProsCons from './ProsCons/ProsCons.astro';
---
<!-- All components that are used in markdown files should be rendered here-->
<!-- This is necessary for Astro to load their CSS classes into the head tag,-->
<!-- since when using them solely in the MDX content files, Astro doesn't load their CSS-->
<div class={wrapper}>
	<CodeBlock code="" />
	<CodeSnippet />
	<Emphasis />
	<Heading />
	<Alert type="info" title="" />
	<GithubRepo repoName=""/>
	<ProsCons pros={[]} cons={[]}/>
	<ItemsList title="" >
		<ItemsListEntry index={0}/>
	</ItemsList>
</div>